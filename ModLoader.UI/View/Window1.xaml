<Window x:Class="ModLoader.UI.View.GameSelectionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ModLoader.UI.View"
        mc:Ignorable="d"
        Title="ModLoader" Height="170" Width="400">
    <Grid>
        <ComboBox x:Name="SellectGame" ItemsSource="{Binding sellectGame}" HorizontalAlignment="Center" Height="31" Margin="0,61,0,0" VerticalAlignment="Top" Width="304"/>
        <Label Content="Игра" HorizontalAlignment="Center" Height="46" Margin="0,10,0,0" VerticalAlignment="Top" Width="290" FontSize="24"/>
        <Button Content="OK" HorizontalAlignment="Center" Height="20" Margin="0,104,0,0" VerticalAlignment="Top" Width="304" Click="Button_Click"/>
    </Grid>
</Window>
